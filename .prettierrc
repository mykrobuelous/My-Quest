{
     "semi": true,
     "trailingComma": "es5",
     "singleQuote": true,
     "printWidth": 100,
     "tabWidth": 5,
     "useTabs": false,
     "arrowParens": "always",
     "endOfLine": "lf",
     "bracketSpacing": true,
     "bracketSameLine": false,
     "htmlWhitespaceSensitivity": "css",
     "embeddedLanguageFormatting": "auto",
     "plugins": ["prettier-plugin-tailwindcss"]
}
